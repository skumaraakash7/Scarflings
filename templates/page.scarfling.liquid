.ribbon {
    position: absolute;
    right: 18px;
    top: 0px;
    z-index: 1;
    overflow: hidden;
    width: 130px;
    height: 130px;
    text-align: right;
    background-color: transparent!important;
}
.ribbon span {
    font-size: 12px !important;
    font-weight: bold;
    color: #FFF;
    text-transform: uppercase;
    text-align: center !important;
    line-height: 1.7em;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    width: 200px;
    display: block;
    background: #79A70A;
    background: #b64d27;
    box-shadow: 0 3px 10px -5px rgba(0, 0, 0, 1);
    position: absolute;
    top: 10px;
    padding-left: 15px;
    right: -55px;
}
.ribbon span::before {
  content: "";
  position: absolute; left: 0px; top: 100%;
  z-index: -1;
  border-left: 3px solid #8F5408;
  border-right: 3px solid transparent;
  border-bottom: 3px solid transparent;
  border-top: 3px solid #8F5408;
}
.ribbon span::after {
  content: "";
  position: absolute; right: 0px; top: 100%;
  z-index: -1;
  border-left: 3px solid transparent;
  border-right: 3px solid #8F5408;
  border-bottom: 3px solid transparent;
  border-top: 3px solid #8F5408;
}
.plan_section .switch {
  position: relative;
  display: inline-block;
  width: 70px;
  height: 34px;
}

.plan_section .switch input { 
  opacity: 0;
  width: 0;
  height: 0;
}

.plan_section .slider {
    position: absolute;
    cursor: pointer;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #b65327;
    -webkit-transition: .4s;
    transition: .4s;
}

.plan_section .slider:before {
  position: absolute;
  content: "";
  height: 26px;
  width: 26px;
  left: 4px;
  bottom: 4px;
  background-color: white;
  -webkit-transition: .4s;
  transition: .4s;
}

.plan_section input:checked + .slider {
  background-color: #2196F3;
}

.plan_section input:focus + .slider {
  box-shadow: 0 0 1px #2196F3;
}

.plan_section input:checked + .slider:before {
  -webkit-transform: translateX(36px);
  -ms-transform: translateX(36px);
  transform: translateX(36px);
}

/* Rounded sliders */
.plan_section .slider.round {
  border-radius: 34px;
}

.plan_section .slider.round:before {
  border-radius: 50%;
}
