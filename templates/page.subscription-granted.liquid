.plan_section .slider {
    position: absolute;
    cursor: pointer;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #b65327;
    -webkit-transition: .4s;
    transition: .4s;
}

.plan_section .slider:before {
  position: absolute;
  content: "";
  height: 26px;
  width: 26px;
  left: 4px;
  bottom: 4px;
  background-color: white;
  -webkit-transition: .4s;
  transition: .4s;
}

.plan_section input:checked + .slider {
  background-color: #2196F3;
}

.plan_section input:focus + .slider {
  box-shadow: 0 0 1px #2196F3;
}

.plan_section input:checked + .slider:before {
  -webkit-transform: translateX(36px);
  -ms-transform: translateX(36px);
  transform: translateX(36px);
}

