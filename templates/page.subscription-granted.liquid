{% include 'query_string_checker' %}
<script>
var encoded = false;
  $(document).ready(function(){
  	
    
    if(encoded){
        var encrypted_email = window.atob(window.queryStrings["email"]);
    	var encrypted_number = window.atob(window.queryStrings["otp"]);
    
    }else{
      	var encrypted_email = window.queryStrings["email"];
    	var encrypted_number =window.queryStrings["otp"];
    }
    
      setTimeout(function(){
        
      
          if(window.queryStrings["email"] != undefined){
            $("#subscription_btn").hide();
            $("#subscription_email").val(encrypted_email);
            $("#subscription_verify_otp").show();
            $("#subscription_verify_otp").trigger("click");
          }
        
        },2000);
  
    });
</script>
